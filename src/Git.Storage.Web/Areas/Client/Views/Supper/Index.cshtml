@{
    Layout = null;
}

<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    @Html.Partial("_Title")
    @Html.Raw(ViewBag.CssFile)
</head>
<body class="fixed-top">
    @Html.Partial("_Top")
    <div id="container" class="row-fluid">
        @Html.Partial("_LeftMenu")
        <div id="body">
            <div id="widget-config" class="modal hide">
                <div class="modal-header">
                    <button data-dismiss="modal" class="close" type="button"></button>
                    <h3>widget Settings</h3>
                </div>
                <div class="modal-body">
                    <p>Here will be a configuration form</p>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row-fluid">
                    <div class="span12">
                        <ul class="breadcrumb">
                            <li>
                                <i class="icon-home"></i>
                                <a href="index.html">首页</a>
                                <i class="icon-angle-right"></i>
                            </li>
                            <li>
                                <a href="javascript:void(0)">基础管理</a>
                                <i class="icon-angle-right"></i>
                            </li>
                            <li><a href="javascript:void(0)">供应商管理</a></li>
                        </ul>
                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <div class="widget box light-grey">
                            <div class="widget-title">
                                <h4><i class="icon-globe"></i>供应商管理</h4>
                                <div class="tools">
                                    <a href="javascript:;" class="collapse"></a>
                                    <a href="#widget-config" data-toggle="modal" class="config"></a>
                                    <a href="javascript:;" class="reload"></a>
                                    <a href="javascript:;" class="remove"></a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="clearfix margin-bottom-10">
                                    @*<div class="btn-group" style="display:none;">
                                      <button class="btn disabled" value="">所有</button>
                                      <button class="btn">在职</button>
                                      <button class="btn">离职</button>
                                    </div>
                                    <div class="btn-group pull-right">
                                        <button class="btn btn-success dropdown-toggle" data-toggle="dropdown">
                                            工具<i class="icon-angle-down"></i>
                                        </button>
                                        <ul class="dropdown-menu">
                                            <li><a href="#">Print</a></li>
                                            <li><a href="#">Save as PDF</a></li>
                                            <li><a href="#">Export to Excel</a></li>
                                        </ul>
                                    </div>*@
                                    <div class="btn-group pull-right" style="margin-right:5px;">
                                        <button id="sample_editable_1_new" class="btn btn-success" onclick="User.Add()">
                                            新增供应商<i class="icon-plus"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="row-fluid" style="border:1px solid #DDDDDD;">
                                    供应商编号&nbsp;<input id="txtUserCode" name="txtUserCode" type="text" class="searchinfo" />&nbsp;&nbsp;
                                    供应商名称&nbsp;<input id="txtUserName" name="txtUserName" type="text" class="searchinfo"/>&nbsp;&nbsp;
                                    
                                    <a id="gritter-remove-all" class="btn btn-info" href="javascript:void(0)" onclick="User.PageClick(1)">搜索</a>
                                </div>
                                <hr />
                                
                                <table class="table table-striped table-bordered table-hover" id="tabInfo">
                                    <thead>
                                        <tr class="widget-title">
                                            <th colspan="10">
                                                <div class="row-fluid" id="mypager">
                                                </div>
                                            </th>
                                        </tr>
                                        <tr class="widget-title">
                                            <th style="width: 8px;"><input type="checkbox" class="group-checkable" onclick="User.SelectAll(this)" /></th>
                                            <th class="hidden-480">供应商编号</th>
                                            <th class="hidden-480">供应商名称</th>
                                            <th class="hidden-480">电话</th>
                                            <th class="hidden-480">传真</th>
                                            <th class="hidden-480">Email</th>
                                            <th class="hidden-480">联系人</th>
                                            <th class="hidden-480">地址</th>
                                            <th class="hidden-480">描述</th>
                                            <th>操作</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("_Foot")
    
    <script type="text/javascript">
        
    </script>
</body>

</html>
